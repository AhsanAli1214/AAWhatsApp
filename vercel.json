{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/sitemap.xml",
      "dest": "server/index.ts"
    },
    {
      "src": "/robots.txt",
      "dest": "server/index.ts"
    },
    {
      "src": "/88eae1744d0947619ec188fec7ff3b06.txt",
      "dest": "server/index.ts"
    },
    {
      "src": "/favicon.png",
      "dest": "favicon.png"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/attached_assets/(.*)",
      "dest": "/attached_assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
